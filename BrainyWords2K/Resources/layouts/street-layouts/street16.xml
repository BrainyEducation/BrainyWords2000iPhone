        android:tag="Animals/wild/sounds/29elephant.mp3"
        android:background="@null"
        android:id="@+id/elephant2"
        android:layout_width="40dp"
        android:layout_height="60dp"
        android:layout_alignBaseline="@+id/giraffes"
        android:layout_alignBottom="@+id/giraffes"
        android:layout_toRightOf="@+id/giraffes"
        android:layout_toEndOf="@+id/giraffes" />

    <Button
        android:onClick="playSound"
